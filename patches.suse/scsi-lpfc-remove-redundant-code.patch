--- a/drivers/scsi/lpfc/lpfc_bsg.c
+++ b/drivers/scsi/lpfc/lpfc_bsg.c
@@ -1569,7 +1569,6 @@ lpfc_issue_ct_rsp(struct lpfc_hba *phba,
