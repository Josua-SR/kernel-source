---
 include/linux/platform_data/mlxreg.h |    3 +--
 1 file changed, 1 insertion(+), 2 deletions(-)

--- a/include/linux/platform_data/mlxreg.h
+++ b/include/linux/platform_data/mlxreg.h
@@ -58,11 +58,10 @@ struct mlxreg_hotplug_device {
